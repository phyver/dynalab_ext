<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <id>dynalab.open_path_expert</id>

    <effect needs-live-preview="false" needs-document="true">
        <effects-menu>
            <submenu name=" Dynalab">
                <submenu name="9 - Expert mode">
                    <submenu name="1 - Diagnostics"/>
                </submenu>
            </submenu>
        </effects-menu>
    </effect>
    <name>4 - Mark paths containing open components</name>

    <param name="only-fill-mode-paths" type="bool" value="true" gui-text="restrict to paths with 'fill mode' color"/>

    <script>
        <command location="inx" interpreter="python">../src/diagnostic_open_paths.py</command>
    </script>
</inkscape-extension>
