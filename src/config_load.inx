<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <id>fablabext.load_config</id>

    <effect needs-live-preview="false">
        <effects-menu>
            <submenu name="00 Fablab Extension">
                <submenu name="expert">
                    <submenu name="configuration"/>
                </submenu>
            </submenu>
        </effects-menu>
    </effect>
    <name>2 - load configuration</name>

    <param name="config-file" type="path" mode="file" gui-text="Load config file:" />

    <script>
        <command location="inx" interpreter="python">config_load.py</command>
    </script>
</inkscape-extension>
